{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
    "api/ping.js": {
      "runtime": "edge"
    }
  },
   "cron": {
    "jobs": [
      {
        "path": "/api/ping",
        "schedule": "*/10 * * * *" 
      }
    ]
  }
}